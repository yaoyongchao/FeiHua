    <style name="s_true" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#33da33</item>
    </style>

    <style name="s_false" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">#9b9b9b</item>
    </style>


    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!--边框-->
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowNoTitle">true</item>
        <!--无标题-->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">true</item>
        <!--模糊-->
    </style>

    <!--自定义seekbarstyle-->
    <style name="CustomSeekbarStyle" >
        <item name="android:maxHeight">20dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@color/colorAccent</item>
        <item name="android:progressDrawable">@drawable/seekbar_upgrade_progress</item>
        <!--<item name="android:progressDrawable">@drawable/progress_upgrade</item>-->
        <item name="android:minHeight">10dp</item>
        <item name="android:thumb">@drawable/seekbar_thumb_selector</item>
        <!--<item name="android:thumb">@drawable/my_thumb</item>-->
    </style>
</resources>
